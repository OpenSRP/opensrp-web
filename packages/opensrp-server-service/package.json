{
    "name": "@opensrp/server-service",
    "version": "0.0.0",
    "description": "Wrapper around OpenSRP Server REST API",
    "main": "dist/index.js",
    "types": "dist/types",
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": "https://github.com/opensrp/opensrp-web",
    "scripts": {
        "jest": "jest --coverage --verbose --color",
        "test": "cd ../../ && yarn test packages/opensrp-server-service --verbose --collectCoverage=true && cd clients/core",
        "tsc": "tsc",
        "transpile": "babel src -d dist --root-mode upward --extensions '.ts,.tsx' --ignore '**/*.test.ts,**/*.test.tsx,**/tests,**/__tests__'"
    },
    "jest": {
        "automock": false,
        "setupFiles": [
            "../../setupTests"
        ]
    },
    "bugs": {
        "url": "https://github.com/opensrp/opensrp-web/issues"
    },
    "author": "Ona Engineering",
    "license": "Apache-2.0",
    "private": false,
    "dependencies": {
        "@onaio/session-reducer": "^0.0.11"
    }
}
